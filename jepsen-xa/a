services:
  db1:
    build:
      context: db
      dockerfile: Dockerfile
  	  args:
        INIT_SQL_SCRIPT: alice.sql
    ports:
      - 55432:5432
    environment:
      POSTGRES_PASSWORD: password
  db2:
    build:
      context: db
      dockerfile: Dockerfile
  	  args:
        INIT_SQL_SCRIPT: bob.sql
    ports:
      - 55433:5432
    environment:
      POSTGRES_PASSWORD: password
